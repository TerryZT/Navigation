{
  "name": "navigation",
  "version": 3,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/node" },
    { "src": "icons/*.svg", "use": "@vercel/static" }
  ],
  "env": {
    "MONGODB_URI": "@mongodb-uri"
  },
  "rewrites": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}